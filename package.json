{
  "name": "sirdez",
  "version": "0.0.0",
  "main": "build/index.js",
  "scripts": {
    "lint": "eslint --fix .",
    "format": "prettier --write .",
    "prepare": "run-s format lint",
    "test": "jest --coverage",
    "verify": "run-p format lint test",
    "badges": "ts-node scripts/badges",
    "clean": "rm -rf dist deno_dist build es_build report coverage",
    "dist:web": "vite build",
    "dist:deno": "denoify --out deno_dist",
    "dist": "run-p dist:*",
    "build:cjs": "tsc",
    "build:esm": "tsc --project tsconfig.es.json",
    "build": "run-p build:*",
    "dist_build": "run-p dist build",
    "doc": "typedoc --out doc src",
    "gen": "run-p dist_build doc"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.7",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "avsc": "^5.7.3",
    "denoify": "^0.10.6",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "jest": "^27.4.5",
    "jest-html-reporters": "^3.0.3",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.4",
    "vite": "^2.7.10"
  }
}
